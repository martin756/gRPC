{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Launch SpringBootKafkaConsumerApplication",
      "request": "launch",
      "mainClass": "com.kafka.springbootkafkaconsumer.SpringBootKafkaConsumerApplication",
      "projectName": "spring-boot-kafka-consumer-pujas"
    },
    {
      "type": "java",
      "name": "Launch KafkaConsumerExample",
      "request": "launch",
      "mainClass": "com.retroshop.KafkaConsumerExample",
      "projectName": "kafka"
    },
    {
      "type": "java",
      "name": "Launch KafkaProducerExample",
      "request": "launch",
      "mainClass": "com.retroshop.KafkaProducerExample",
      "projectName": "kafka"
    },
    {
      "name": ".NET Core Launch (web)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/grpc_client/bin/Debug/net5.0/apiRetroshop.dll",
      "args": [],
      "cwd": "${workspaceFolder}/grpc_client",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}
